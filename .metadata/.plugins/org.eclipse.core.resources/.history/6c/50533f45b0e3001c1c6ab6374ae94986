package com.extreme.cursojava.aula11;

public class CuriosidadesInt {

	public static void main(String[] args) {

		int var1 = 2147483647;
		int var2 = 1;
		
		System.out.println(var1 + var2);
		
		//Os números no java funcionam igual a uma roleta
		//Quando eu pego o limite e faço a soma com outro número
		//ele volta para os números negativos
	}

}
